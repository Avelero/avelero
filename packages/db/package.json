{
  "name": "@v1/db",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "typecheck": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "cd ../../apps/api && supabase migration up --local",
    "db:backfill:subscription": "bun run src/scripts/backfill-brand-subscription-foundations.ts",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit introspect",
    "db:studio": "drizzle-kit studio",
    "db:check": "drizzle-kit check",
    "db:up": "drizzle-kit up",
    "test": "bun test __tests__ --serial --timeout 30000"
  },
  "exports": {
    "./client": "./src/client.ts",
    "./schema": "./src/schema/index.ts",
    "./queries": "./src/queries/index.ts",
    "./queries/products": "./src/queries/products/index.ts",
    "./queries/catalog": "./src/queries/catalog/index.ts",
    "./queries/brand": "./src/queries/brand/index.ts",
    "./queries/user": "./src/queries/user/index.ts",
    "./queries/taxonomy": "./src/queries/taxonomy/index.ts",
    "./queries/dpp": "./src/queries/dpp/index.ts",
    "./queries/integrations": "./src/queries/integrations/index.ts",
    "./queries/bulk": "./src/queries/bulk/index.ts",
    "./queries/notifications": "./src/queries/notifications/index.ts",
    "./defaults": "./src/defaults/index.ts",
    "./index": "./src/index.ts",
    "./utils": "./src/utils/index.ts",
    "./testing": "./src/testing/index.ts"
  },
  "dependencies": {
    "@v1/dpp-components": "workspace:*",
    "drizzle-orm": "^0.44.7",
    "postgres": "^3.4.7"
  },
  "devDependencies": {
    "@types/bun": "^1.2.16",
    "@types/node": "^24.0.1",
    "@v1/supabase": "workspace:*",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.7",
    "typescript": "^5.9.2"
  }
}
