{
  "name": "@v1/integrations",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./src/index.ts"
  },
  "scripts": {
    "lint": "biome lint ./src",
    "typecheck": "tsc --noEmit",
    "test": "bun test --preload ./__tests__/setup.ts --serial --timeout 30000",
    "test:unit": "bun test ./__tests__/unit --timeout 10000",
    "test:integration": "bun test --preload ./__tests__/setup.ts --serial --timeout 30000 --testPathPattern='\\.integration\\.test\\.ts$'",
    "test:watch": "bun test --watch --preload ./__tests__/setup.ts"
  },
  "dependencies": {
    "@v1/db": "workspace:*",
    "@v1/supabase": "workspace:*"
  },
  "devDependencies": {
    "@types/bun": "^1.2.16",
    "@types/node": "^24.0.1",
    "@v1/testing": "workspace:*",
    "@v1/tsconfig": "workspace:*",
    "dotenv": "^17.2.3",
    "typescript": "^5.9.2"
  }
}