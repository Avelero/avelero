    "@v1/email": "workspace:*",
    "@v1/integrations": "workspace:*",
    "@v1/supabase": "workspace:*",
    "@v1/utils": "workspace:*",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.7",
    "exceljs": "^4.4.0",
    "import-in-the-middle": "^2.0.0",
    "jschardet": "^3.1.4",
    "p-map": "^7.0.2",
    "papaparse": "^5.5.3",
    "react": "19.2.1",
    "resend": "^6.4.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@trigger.dev/build": "4.0.6",
