    "./trigger/commit-to-production": "./src/trigger/commit-to-production.ts",
    "./trigger/capture-theme-screenshot": "./src/trigger/capture-theme-screenshot.ts",
    "./trigger/delete-brand": "./src/trigger/delete-brand.ts",
    "./trigger/integrations": "./src/trigger/integrations/index.ts",
    "./trigger/integrations/sync": "./src/trigger/integrations/sync.ts",
    "./trigger/integrations/scheduler": "./src/trigger/integrations/scheduler.ts"
  }
