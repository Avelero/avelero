{
  "name": "@v1/jobs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "dev": "trigger dev",
    "lint": "biome check .",
    "deploy": "npx --yes trigger.dev@4.0.6 deploy",
    "login": "npx --yes trigger.dev@4.0.6 login",
    "whoami": "npx --yes trigger.dev@4.0.6 whoami",
    "format": "biome format --write .",
    "typecheck": "tsc --noEmit",
    "test": "bun run test:unit && bun run test:integration",
    "test:unit": "bun test __tests__/unit --timeout 10000",
    "test:integration": "bun test __tests__/integration --serial --timeout 30000",
    "test:watch": "bun test __tests__ --watch",
    "test:coverage": "bun test __tests__ --coverage"
  },
  "dependencies": {
    "@react-email/render": "^2.0.1",
    "@supabase/supabase-js": "^2.57.2",
    "@trigger.dev/sdk": "4.0.6",
